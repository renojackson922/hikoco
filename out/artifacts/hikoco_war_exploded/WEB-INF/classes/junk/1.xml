<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

        <!--useGeneratedKeys="true" keyProperty="mem_id">-->
        <!--(mem_userid, mem_password, mem_realname, mem_nickname, mem_photo, mem_tier, mem_phone, mem_birthday, mem_sex, mem_zipcode, mem_addr1, mem_addr2, mem_addr3, mem_addr4)-->
        <!-- DAO(Interface) 아래 resultType에는 DTO 지정 -->
<mapper namespace="org.silkdog.maven.hikoco.member.dao.MemberDAO">
    <insert id="insert" parameterType="org.silkdog.maven.hikoco.member.dto.MemberDTO">
      <selectKey keyProperty="mem_id" resultType="int" order="BEFORE">
        select HIKOCO_MEMBER_SEQ.nextval FROM DUAL
      </selectKey>
      INSERT INTO HIKOCO_MEMBER (mem_userid, mem_password, mem_realname, mem_nickname, mem_photo, mem_tier, mem_phone, mem_birthday, mem_sex, mem_zipcode, mem_addr1, mem_addr2, mem_addr3, mem_addr4) VALUES (#{mem_userid},
                                          #{mem_password},
                                          #{mem_realname},
                                          #{mem_nickname},
                                          #{mem_photo},
                                          #{mem_tier},
                                          #{mem_phone},
                                          #{mem_birthday},
                                          #{mem_sex},
                                          #{mem_zipcode},
                                          #{mem_addr1},
                                          #{mem_addr2},
                                          #{mem_addr3},
                                          #{mem_addr4})
    </insert>
</mapper>