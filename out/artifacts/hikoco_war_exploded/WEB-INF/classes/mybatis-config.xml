        <typeAlias type="org.silkdog.maven.simpleboard.board.vo.BoardVO" alias="boardVO"/>
    </typeAliases>
    <mappers>
        <!-- Hikoco -->
        <mapper resource="mybatis/MemberMapper.xml"></mapper>
        <mapper resource="mybatis/ItemMapper.xml"></mapper>
        <mapper resource="mybatis/ItemOnelineMapper.xml"></mapper>
        <mapper resource="mybatis/ItemCatMapper.xml"></mapper>
        <mapper resource="mybatis/MyCartMapper.xml"></mapper>
